{
    "name": "<%= @calico_name %>",
    "type": "calico",
    "etcd_endpoints": "<% array=[] -%>
<% @etcd_count.to_i.times do |i| -%>
<%  array<<"http://etcd-#{i}.#{@ec2_tag_cluster}.#{@dns_root}:#{@calico_etcd_port}" -%>
<% end -%>
<%= array.join(',') %>",
    "log_level": "info",
    "ipam": {
        "type": "calico-ipam"
    },
    "policy": {
        "type": "k8s"
    },
    "kubernetes": {
        "kubeconfig": "/etc/kubernetes/kubeconfig"
    }
}
